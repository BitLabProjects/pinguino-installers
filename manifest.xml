<?xml version="1.0" encoding="UTF-8"?>

<manifest>
    
    <remote 
        name="github" 
        fetch="https://github.com/" />    
    
    <default    
        revision="master"
        remote="github" 
        clone-depth="1" 
        sync-c="true"
        sync-j="4"/>
    
    <project
        path="ide" 
        name="PinguinoIDE/pinguino-ide" />
    
    <project 
        path="libraries" 
        name="PinguinoIDE/pinguino-libraries" />
    
    <project 
        path="extra" 
        name="PinguinoIDE/pinguino-extra" />
        
    <project 
        path="bootloaders" 
        name="PinguinoIDE/pinguino-bootloaders" />

    <project 
        path="multilanguage" 
        name="PinguinoIDE/pinguino-multilanguage" />
    
    <project 
        path="compilers" 
        name="PinguinoIDE/pinguino-compilers" />
    
</manifest>